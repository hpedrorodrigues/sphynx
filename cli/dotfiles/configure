#!/usr/bin/env bash

set -o errexit
set -o nounset
set -o pipefail

source "${SPHYNX_COMMAND_DIR}/.internal/all.sh"
source "${SPHYNX_NAMESPACE_DIR}/shared/shared.sh"
source "${SPHYNX_NAMESPACE_DIR}/core/configure.sh"

##? Configure dot files in home directory
##?
##? Usage:
##?     configure
##?
##? Examples:
##?     configure

sx::parse_arguments "${@}"

sx::dotfiles::configure
