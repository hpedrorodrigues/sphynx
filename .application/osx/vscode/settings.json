{
  "breadcrumbs.enabled": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "line-thin",
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.minimap.maxColumn": 100,
  "editor.minimap.renderCharacters": false,
  "editor.renderWhitespace": "all",
  "editor.rulers": [80, 100, 120],
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "files.associations": {
    ".gitignore_global": "ignore",
    ".gitsecrets": "properties"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.idea": true,
    "**/build": true,
    "**/node_modules": true,
    "**/target": true,
    "**/vendor": true,
    "**/__pycache__": true
  },
  "files.trimTrailingWhitespace": true,
  "search.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.idea": true,
    "**/build": true,
    "**/node_modules": true,
    "**/target": true,
    "**/vendor": true,
    "**/__pycache__": true
  },
  "vsintellicode.modify.editor.suggestSelection": "choseToUpdateConfiguration",
  "workbench.colorCustomizations": {
    "[Material Theme Darker]": {
      "list.activeSelectionForeground": "#dfe2e2",
      "list.inactiveSelectionForeground": "#dfe2e2",
      "sideBar.foreground": "#738598"
    }
  },
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.welcome.enabled": false,
  "window.zoomLevel": 0,
  "go.autocompleteUnimportedPackages": true,
  "go.enableCodeLens": true,
  "cSpell.language": "en,pt-BR",
  "redhat.telemetry.enabled": true,
  "go.toolsManagement.autoUpdate": true,
  "workbench.colorTheme": "Coldark - Dark",
  "workbench.iconTheme": "deepdark-material-icons"
}
